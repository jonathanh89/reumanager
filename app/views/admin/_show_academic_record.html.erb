<div class="span-17">
		<%= h(f.major) %>
		<%= h(f.college_level) %>&nbsp;&nbsp;<b>AT</b>&nbsp;
		<%= h(f.college) %>&nbsp;
		(<%= h(f.college_start.to_date.strftime("%Y.%m")) %>&nbsp;-&nbsp;<%= h(f.college_end.to_date.strftime("%Y.%m")) %>)
		<%= @spacer %>
		<label>GPA</label>&nbsp;<%= h(f.gpa) %>&nbsp;/&nbsp;<%= h(f.gpa_range) + @spacer %>
</div>

<div class="span-17 last transcript">
	<label>TRANSCRIPT</label>&nbsp;
	<%- if @user.id < 6 -%>
		<%= link_to " #{@user.academic_record.transcript.original_filename}", ((ActionController::Base.relative_url_root || '') + '/system/transcripts/1/original/3_cbcff5c4b00739c2bf0b4b923009e73fb69e194b863dab0d.pdf'), :class => "ss_sprite ss_page_white_acrobat" %>
	<%- else -%>
		<%= link_to " #{@user.academic_record.transcript.original_filename}", ((ActionController::Base.relative_url_root || '') + @user.academic_record.transcript.url), :class => "ss_sprite ss_page_white_acrobat" %>
	<%- end -%>
	
	<% if f.p_college != "" && f.p_college != nil -%>
		<br /><label>PREVIOUS UNIVERSITY</label>&nbsp;<%= h(f.p_college) %>
		(<%= h(f.p_college_start.to_date.strftime("%Y.%m")) %>&nbsp;-&nbsp;
		<%= h(f.p_college_end.to_date.strftime("%Y.%m")) %>)
	<% end -%>
</div>